#define _CRT_SECURE_NO_WARNINGS
#include<bits/stdc++.h>
using namespace std;
void fast()
{
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr), cout.tie(nullptr);
}
void solve()
{
	int N, M, K, V; 
	while (cin >> N >> M)
	{
		map<int, int>ma;
		vector <vector<pair<int, int>>>ve(N + 1);
		for (int i = 0; i < N; i++)
		{
			cin >> V;
			ma[V]++;
			ve[ma[V]].push_back({ V,i + 1 });
		}
		for (int i = 1; i <= N; i++)
			sort(all(ve[i]));
		while (M--)
		{
			cin >> K >> V;
			if (ma[V] < K)
				cout << "0\n";
			else
			{
				int l = 0, r = ve[K].size(), mid, ans = 0;
				while (l <= r)
				{
					mid = ((l + r) >> 1);
					if (ve[K][mid].first == V)
					{
						ans = ve[K][mid].second;
						break;
					}
					else if (ve[K][mid].first < V)
						l = mid + 1;
					else
						r = mid - 1;
				}
				cout << ans << "\n";
			}
		}
	}
}
int main()
{
	fast();
	int T = 1; //cin >> T;
	while (T--)
		solve();
}
